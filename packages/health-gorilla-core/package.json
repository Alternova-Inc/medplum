{
  "name": "@medplum/health-gorilla-core",
  "version": "0.0.8",
  "private": true,
  "description": "Medplum Health Gorilla SDK",
  "sideEffects": false,
  "exports": {
    "import": {
      "types": "./dist/esm/index.d.ts",
      "default": "./dist/esm/index.mjs"
    },
    "require": {
      "types": "./dist/cjs/index.d.ts",
      "default": "./dist/cjs/index.cjs"
    }
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/cjs/index.d.ts",
  "files": [
    "dist/cjs",
    "dist/esm"
  ],
  "scripts": {
    "api-extractor": "api-extractor run --local && cp dist/types.d.ts dist/cjs/index.d.ts && cp dist/types.d.ts dist/esm/index.d.ts",
    "build": "npm run clean && tsc && node esbuild.mjs && npm run api-extractor",
    "clean": "rimraf dist",
    "lint": "eslint src/",
    "pack": "npm run build && npm pack",
    "test": "vitest"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": [
      "@medplum/eslint-config"
    ]
  },
  "devDependencies": {
    "@medplum/core": "3.2.8",
    "@medplum/definitions": "3.2.8",
    "@medplum/eslint-config": "3.2.8",
    "@medplum/fhirtypes": "3.2.8",
    "@medplum/mock": "3.2.8",
    "@microsoft/api-extractor": "7.47.7",
    "@types/node": "20.14.12",
    "vitest": "2.0.5"
  },
  "peerDependencies": {
    "@medplum/core": "^3.2.2",
    "@medplum/fhirtypes": "^3.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
